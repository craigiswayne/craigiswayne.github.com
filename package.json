{
  "name": "craigiswayne.github.com",
  "version": "1.0.0",
  "description": "Personal Blog hosted on github using github pages",
  "main": "index.js",
  "scripts": {
    "start": "bundle exec jekyll serve",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "copy-files-from-to",
    "lighthouse-ci": "npm run 'test-lighthouse-mobile' http://localhost:4000 && npm run 'test-lighthouse-desktop' http://localhost:4000",
    "lighthouse": "npm run 'verbose-test-lighthouse-mobile' http://localhost:4000 && npm run 'verbose-test-lighthouse-desktop' http://localhost:4000",
    "test-lighthouse-mobile": "lighthouse-ci $1 --budget-path=budget.json --chrome-flags='--headless --allow-insecure-localhost --ignore-certificate-errors' --score=75 --performance=75 --accessibility=75 --best-practices=75 --seo=75 --pwa=75",
    "test-lighthouse-desktop": "lighthouse-ci $1 --budget-path=budget.json --chrome-flags='--headless --allow-insecure-localhost --ignore-certificate-errors' --score=75 --performance=75 --accessibility=75 --best-practices=75 --seo=75 --pwa=75 --emulated-form-factor=desktop",
    "verbose-test-lighthouse-mobile": "lighthouse $1 --quiet --no-enable-error-reporting --budget-path=budget.json --save-assets=false --chrome-flags='--headless --allow-insecure-localhost --ignore-certificate-errors'",
    "verbose-test-lighthouse-desktop": "lighthouse $1 --quiet --no-enable-error-reporting --budget-path=budget.json --save-assets=false --chrome-flags='--headless --allow-insecure-localhost --ignore-certificate-errors' --emulated-form-factor=desktop"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/craigiswayne/craigiswayne.github.com.git"
  },
  "keywords": [
    "jekyll",
    "github",
    "pages"
  ],
  "author": "Craig Wayne",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/craigiswayne/craigiswayne.github.com/issues"
  },
  "homepage": "https://github.com/craigiswayne/craigiswayne.github.com#readme",
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "copy-files-from-to": "^2.1.0",
    "jquery": "^3.5.0",
    "jquery-easing": "0.0.1",
    "lighthouse-ci": "^1.10.0",
    "vue": "^2.6.11",
    "vue-chartkick": "^0.6.0"
  },
  "copyFiles": [
    {
      "from": "node_modules/bootstrap/dist/css/*",
      "to": "assets/vendor/bootstrap/css/"
    },
    {
      "from": "node_modules/bootstrap/dist/js/*",
      "to": "assets/vendor/bootstrap/js/"
    },
    {
      "from": "node_modules/@fortawesome/fontawesome-free/css/*",
      "to": "assets/vendor/fontawesome-free/css/"
    },
    {
      "from": "node_modules/@fortawesome/fontawesome-free/webfonts/*",
      "to": "assets/vendor/fontawesome-free/webfonts/"
    },
    {
      "from": "node_modules/vue/dist/*.js",
      "to": "assets/vendor/vue/"
    },
    {
      "from": "node_modules/chart.js/dist/*",
      "to": "assets/vendor/chart.js/"
    },
    {
      "from": "node_modules/vue-chartkick/dist/*",
      "to": "assets/vendor/vue-chartkick/"
    },
    {
      "from": "node_modules/jquery/dist/*",
      "to": "assets/vendor/jquery/"
    },
    {
      "from": "node_modules/jquery-easing/*.js",
      "to": "assets/vendor/jquery-easing/"
    }
  ]
}
